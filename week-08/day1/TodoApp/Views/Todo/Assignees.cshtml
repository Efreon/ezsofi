@using EntityFramework.Models.ViewModels
@model AssigneeViewModel
@{
    var assignees = Model.Assignees;
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Todo List</title>
</head>
<body>
    <div class="todos">
        <div class="title">
            <h1>Todos for today:</h1>
        </div>
        <div class="todo-list">
            <ul id="todos">
                @foreach (var assignee in assignees)
                {
                    <li>@assignee.Name</li>
                }
            </ul>
        </div>
    </div>
</body>
</html>
