@using MyShop.Models.ViewModels
@model ItemViewModel
@{
    var shopItems = Model.ShopItems;
}
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Price</th>
            <th>Quantity of Stock </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in shopItems)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.PriceEur</td>
                <td>@item.QuantityOfStock</td>
            </tr>
        }
    </tbody>
</table>
